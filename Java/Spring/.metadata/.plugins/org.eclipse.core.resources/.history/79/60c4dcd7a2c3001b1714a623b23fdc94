package com.sayan.SpringDemo;

public class Alien {
	private int age;
	private Laptop lap;
	public Alien(int age) {
		this.age = age;
	}
	public Laptop getLap() {
		return lap;
	}
	public void setLap(Laptop lap) {
		this.lap = lap;
	}
	public Alien() {
		System.out.println("Alien object created.");
	}
	public void code() {
		System.out.println("I am coding");
		lap.compile();
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
}
